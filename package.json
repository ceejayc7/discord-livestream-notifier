{
  "name": "discord-livestream-notifier",
  "version": "1.0.0",
  "description": "Notify discord channels when livestream has gone live",
  "main": "src/discord.js",
  "author": "Ceejay",
  "scripts": {
    "start": "node dist/discord.js",
    "build": "babel src/ -d dist/ --copy-files",
    "watch": "babel -w src/ -d dist/ --copy-files",
    "debug": "babel-node --inspect-brk src/discord.js --presets es2015,stage-0"
  },
  "engines": {
    "node": "8.11.3"
  },
  "dependencies": {
    "card-deck": "^1.0.0",
    "discord.js": "^11.3.2",
    "lodash": "^4.17.10",
    "request": "^2.87.0",
    "uws": "^10.148.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.7",
    "nodemon": "^1.18.2"
  }
}
