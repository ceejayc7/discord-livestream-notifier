{
  "name": "discord-livestream-notifier",
  "version": "1.0.0",
  "description": "Notify discord channels when livestream has gone live",
  "main": "src/discord.js",
  "author": "Ceejay",
  "scripts": {
    "start": "node dist/discord.js",
    "build": "eslint src/**/*.js && babel src -d dist --copy-files",
    "watch": "concurrently \"npm:watch:babel\" \"npm:watch:eslint\"",
    "watch:babel": "babel --watch src -d dist --copy-files",
    "watch:eslint": "esw --w src/**/*.js",
    "debug": "babel-node --inspect-brk src/discord.js"
  },
  "engines": {
    "node": "12.16.1"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    }
  },
  "dependencies": {
    "card-deck": "1.0.1",
    "cheerio": "1.0.0-rc.3",
    "discord.js": "12.0.2",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-timezone": "0.5.28",
    "node-json-db": "0.7.5",
    "prob.js": "1.0.6",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "twitter": "1.7.1",
    "uws": "10.148.1",
    "weighted-random": "0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-watch": "6.0.1"
  }
}
