{
  "name": "discord-livestream-notifier",
  "version": "1.0.0",
  "description": "Notify discord channels when livestream has gone live",
  "main": "src/discord.js",
  "author": "Ceejay",
  "scripts": {
    "start": "node dist/discord.js",
    "build": "eslint src/**/*.js && babel src/ -d dist/ --copy-files",
    "watch": "concurrently \"npm:watch:babel\" \"npm:watch:eslint\"",
    "watch:babel": "babel --watch src/ -d dist/ --copy-files",
    "watch:eslint": "esw --w src/**/*.js",
    "debug": "babel-node --inspect-brk src/discord.js"
  },
  "engines": {
    "node": "10.16.0"
  },
  "dependencies": {
    "card-deck": "^1.0.0",
    "cheerio": "^1.0.0-rc.2",
    "discord.js": "^11.3.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "node-json-db": "^0.7.5",
    "prob.js": "^1.0.6",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "twitter": "^1.7.1",
    "uws": "^10.148.1",
    "weighted-random": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "concurrently": "^4.1.2",
    "eslint": "^6.2.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-watch": "^6.0.0"
  }
}
