{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10"
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
      "module-resolver",
      {
        "alias": {
          "@root": "./src",
          "@stream": "./src/stream",
          "@iptv": "./src/iptv"
        }
      }
    ]
  ]
}
